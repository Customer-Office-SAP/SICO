{
  "name": "sico",
  "version": "1.0.0",
  "description": "Proyecto SICO con CAP y Fiori",
  "scripts": {
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml",
    "build-local": "ui5 build preload --clean-dest",
    "start": "ui5 serve",
    "build:cf": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@ui5/cli": "^4.0.16",
    "@sap/ux-ui5-tooling": "1",
    "ui5-task-zipper": "^3"
  },
  "keywords": [
    "sap",
    "cap",
    "fiori",
    "sico"
  ],
  "main": "webapp/index.html",
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  }
}
